# Add inputs and outputs from these tool invocations to the build variables 

# All Target
all: timedwakeup.axf

# Tool invocations
timedwakeup.axf: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: MCU Linker'
	arm-none-eabi-gcc -nostdlib -Xlinker -Map=timedwakeup.map -Xlinker --gc-sections -mcpu=cortex-m0 -mthumb -T "../linker/lpc1114_flash_crp.ld" -o "timedwakeup.axf" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '
	$(MAKE) --no-print-directory post-build

# Other Targets
clean:
	-$(RM) $(EXECUTABLES)$(OBJS)$(C_DEPS) timedwakeup.axf
	-@echo ' '

post-build:
	-@echo 'Performing post-build steps'
	-arm-none-eabi-size timedwakeup.axf; arm-none-eabi-objcopy -O binary timedwakeup.axf timedwakeup.bin ; checksum -p LPC1114_302 -d timedwakeup.bin;
	-@echo ' '

.PHONY: all clean dependents
.SECONDARY: post-build

